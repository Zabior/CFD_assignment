FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.17748239349298847 0 0);

boundaryField
{   
    Inlet 
    {
      type              inletOutlet;
      inletValue         $internalField;
    }

    Outlet
    {
      type              zeroGradient;
    }


    Side
    {   
        type            empty;
    }

    Cylinder
    {
        type            noSlip;
    }

    Wall
    {
        // type            movingWallVelocity;
        // value           $internalField;
        type            translatingWallVelocity;
        U           $internalField;
    }
}


// ************************************************************************* //
