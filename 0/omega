FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

// internalField   uniform 4.08248290463863;
internalField   uniform 4.83045891539648 ; 
boundaryField
{
    Inlet
    {
    //   type              turbulentMixingLengthFrequencyInlet;
    //   //type turbulentMixingLengthDissipationRateInlet;
    //   //mixingLength 0.00045;
    //   mixingLength 0.0008216618978461462;
    //   value        $internalField;

    type inletOutlet;
    inletValue $internalField;
    
    }

    Outlet
    {
      type              zeroGradient;
    }


    Side
    {
        type            empty;
    }

    Cylinder
    {
        type            omegaWallFunction;
        blended         true;
        value           $internalField;
    }

    Wall
    {
        type            omegaWallFunction;
        blended         true;
        value           $internalField;
    }
        Top
    {
    //     type              turbulentMixingLengthFrequencyInlet;
    //   mixingLength 0.0045;
    //   value        $internalField;
        //   type              zeroGradient;
    type slip;
    }
}


