FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          GAMG;
        smoother        GaussSeidel;
        tolerance       1e-6;
        relTol          0.01;
    }

    "(U|e|epsilon|R)"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-8;
        relTol          0.01;
    }

    UFinal {
        relTol 0.0;
    }
    RFinal {
        relTol 0.0;
    }
}

SIMPLE
{
    residualControl
    {
        p               1e-5;
        U               1e-5;
        "(k|epsilon|e|R)"   1e-5;
    }

    nNonOrthogonalCorrectors 1;
    pMinFactor      0.1;
    pMaxFactor      2;
}

relaxationFactors
{
    fields
    {
        p               0.5;
        rho             0.01;
    }
    equations
    {
        U               0.3;
        e               0.3;
        epsilon     0.3;
        // p 0.1;
        R 0.3;
        pFinal 1;
        RFinal 1;
        // rho 0.01;
    }
}
